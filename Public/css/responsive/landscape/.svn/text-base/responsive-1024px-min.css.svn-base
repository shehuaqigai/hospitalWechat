/*small android pads with height is 600*/
@media all
and (min-width : 1024px)
and (max-height : 700px)
and (orientation : landscape) {
    .page .page-header .page-header-content .logo {
        left: 40px;
    }
    .page .page-header .page-header-content .user-login {
        float: left;
        margin: 55px 0px 0 864px;
        cursor: pointer;
    }
    .page .page-region {
        top: 50px;
        padding: 0 2px 24px 37px;
    }
    .tile-group-title{
        margin-bottom: 0px;
    }

    .tile-group .tile-group-title {
        top: 20px;
        margin-bottom: 24px;
    }
    .dialog-container {
        width: 33%;
        height: 49%;
        display: block;
        position:relative;
        overflow: visible;
        margin: 20% 30% 20% 30%;
    }
}
